{
  "name": "footbook",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:headless": "ng test --code-coverage --progress false --watch false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:headless": "ng e2e  --protractorConfig=e2e/protractor.headless.conf.js",
    "generate:documentation": "compodoc -p tsconfig.app.json",
    "before:push": "run-p build:prod test:headless e2e:headless lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "angular-ng-autocomplete": "^2.0.1",
    "bulma": "^0.9.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.11.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~5.20.1",
    "typescript": "~4.0.3"
  }
}
